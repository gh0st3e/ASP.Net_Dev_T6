@model IEnumerable<PhoneDictionary.Phone>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Header {
    <h2>Список телефонов (Header)</h2>
}

<p><a href="/Dict/Add">Добавить</a></p>
<div class="phonesCardArea">
    @foreach (var item in Model)
    {
        @Html.Partial("PhoneItem", (object)item)
    }
</div>

@section Footer {
    <p>&copy; @DateTime.Now.Year - моя Lab6_Index (Footer)</p>
}
